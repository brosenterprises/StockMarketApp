# Stock Market API Keys
EXPO_PUBLIC_ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
EXPO_PUBLIC_IEX_CLOUD_API_KEY=your_iex_cloud_api_key_here
EXPO_PUBLIC_FINNHUB_API_KEY=your_finnhub_api_key_here
EXPO_PUBLIC_NEWS_API_KEY=your_news_api_key_here

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_API_BASE_URL=https://api.example.com

# Analytics (Optional)
EXPO_PUBLIC_ANALYTICS_KEY=your_analytics_key_here

# Push Notifications (Optional)
EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=your_push_key_here

# Instructions:
# 1. Copy this file to .env
